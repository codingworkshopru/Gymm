<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SatelliteProgressBar">
        <attr name="startAngle" format="float" />
        <attr name="mainColor" format="color" />
        <attr name="secondaryColor" format="color" />
        <attr name="angle" format="float" />
        <attr name="satelliteDiameter" format="dimension" />
        <attr name="orbitStrokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomisableNumberPicker">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="step" format="integer" />
    </declare-styleable>
</resources>